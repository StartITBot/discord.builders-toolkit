    <%_ %><% if ((data.comp.min_values && data.comp.min_values !== 1) || (data.comp.max_values && data.comp.max_values !== 1)) { %>, max_values=<%=data.comp.max_values || 1%><% } %><% _%>
    <%_ %>)<% data.comp.options.forEach(function(comp){ -%>.add_option(
